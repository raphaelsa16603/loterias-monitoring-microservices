{
  "Redis": {
    "Host": "redis",
    "Port": "6379",
    "DefaultTTLMinutes": 360
  },
  "CaixaApi": {
    "BaseUrl": "https://servicebus2.caixa.gov.br/portaldeloterias/api/",
    "TimeoutSeconds": 15,
    "RetryCount": 3,
    "EnableLogging": true
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*"
}
