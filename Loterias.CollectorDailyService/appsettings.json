{
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "BaseTopicName": "loterias",
    "RetryCount": 3,
    "PublishTimeoutMs": 3000
  },
  "Collector": {
    "ExecucoesDiarias": 3,
    "TiposDeJogo": [
      "megasena",
      "lotofacil",
      "quina",
      "lotomania",
      "timemania",
      "diadesorte",
      "duplasena",
      "supersete",
      "maismilionaria",
      "federal",
      "loteca"
    ],
    "Endpoints": {
      "CaixaApi": "http://loterias-caixa-api:8080/",
      "QueryApi": "http://loterias-query-api:8080/"
    }
  },
  "Logging": {
    "MinimumLevel": "Information"
  }
}
